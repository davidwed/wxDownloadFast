                                        <option>1</option>
                                        <flag>wxEXPAND</flag>
                                        <object class="wxBoxSizer">
                                            <orient>wxVERTICAL</orient>
                                            <object class="spacer">
                                                <size>20, 5</size>
                                            </object>
                                            <object class="sizeritem">
                                                <object class="wxTextCtrl" name="edtcomments">
                                                    <style>wxTE_MULTILINE</style>
                                                    <size>210, 70</size>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="spacer">
                                <size>10, 20</size>
                            </object>
                            <object class="spacer">
                                <size>10, 20</size>
                            </object>
                            <object class="sizeritem">
                                <option>1</option>
                                <object class="wxStaticBoxSizer">
                                    <orient>wxHORIZONTAL</orient>
                                    <label>Start Download</label>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <flag>wxEXPAND</flag>
                                        <object class="wxFlexGridSizer">
                                            <hgap>0</hgap>
                                            <rows>7</rows>
                                            <cols>2</cols>
                                            <vgap>0</vgap>
                                            <object class="spacer">
                                                <size>20, 5</size>
                                            </object>
                                            <object class="spacer">
                                                <size>85, 5</size>
                                            </object>
                                            <object class="sizeritem">
                                                <object class="wxRadioButton" name="optmanual">
                                                    <label>Manual</label>
                                                    <value>1</value>
                                                </object>
                                            </object>
                                            <object class="spacer">
                                                <size>20, 5</size>
                                            </object>
                                            <object class="spacer">
                                                <size>20, 5</size>
                                            </object>
                                            <object class="spacer">
                                                <size>20, 5</size>
                                            </object>
                                            <object class="sizeritem">
                                                <object class="wxRadioButton" name="optnow">
                                                    <label>Now</label>
                                                </object>
                                            </object>
                                            <object class="spacer">
                                                <size>20, 5</size>
                                            </object>
                                            <object class="spacer">
                                                <size>20, 5</size>
                                            </object>
                                            <object class="spacer">
                                                <size>20, 5</size>
                                            </object>
                                            <object class="sizeritem">
                                                <object class="wxRadioButton" name="optschedule">
                                                    <label>Schedule</label>
                                                </object>
                                            </object>
                                            <object class="spacer">
                                                <size>20, 20</size>
                                            </object>
                                            <object class="spacer">
                                                <size>20, 15</size>
                                            </object>
                                            <object class="spacer">
                                                <size>20, 5</size>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <option>1</option>
                        <flag>wxEXPAND|wxALIGN_RIGHT</flag>
                        <object class="wxFlexGridSizer">
                            <hgap>0</hgap>
                            <rows>3</rows>
                            <cols>1</cols>
                            <vgap>0</vgap>
                            <object class="spacer">
                                <size>20, 10</size>
                            </object>
                            <object class="sizeritem">
                                <option>1</option>
                                <flag>wxALIGN_RIGHT</flag>
                                <object class="wxFlexGridSizer">
                                    <hgap>0</hgap>
                                    <rows>1</rows>
                                    <cols>5</cols>
                                    <vgap>0</vgap>
                                    <object class="spacer">
                                        <size>250, 20</size>
                                    </object>
                                    <object class="sizeritem">
                                        <object class="wxButton" name="btnok">
                                            <label>_Ok</label>
                                        </object>
                                    </object>
                                    <object class="spacer">
                                        <size>20, 20</size>
                                    </object>
                                    <object class="sizeritem">
                                        <object class="wxButton" name="btncancel">
                                            <label>_Cancel</label>
                                        </object>
                                    </object>
                                    <object class="spacer">
                                        <size>10, 20</size>
                                    </object>
                                    <object class="spacer">
                                        <size>20, 10</size>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>
